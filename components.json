{
	"$schema": "https://ui.shadcn.com/schema.json",
	"style": "new-york",
	"rsc": true,
	"tsx": true,
	"tailwind": {
		"config": "tailwind.config.js",
		"css": "src/app/(client)/globals.css",
		"baseColor": "gray",
		"cssVariables": true,
		"prefix": ""
	},
	"aliases": {
		"components": "src/app/(client)/components",
		"utils": "@lib/utils",
		"ui": "src/shadcn-ui",
		"lib": "src/lib",
		"hooks": "src/app/(client)/hooks/ui",
		"services": "src/app/(server)/services",
		"types": "src/types",
		"providers": "src/app/(client)/providers"
	},
	"iconLibrary": "lucide"
}
